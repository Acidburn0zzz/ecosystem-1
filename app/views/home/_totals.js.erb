Rails.ajax({
  url: "/versions/<%= CGI.escape(key) %>?total=true",
  type: "get",
  success: series => {
    const options = {
      stroke: {
        width: 2
      },
      chart: {
        id: "totals-chart-versions-<%= CGI.escape(key) %>",
        height: 400,
        type: "bar",
        animations: { enabled: false }
      },
      series,
      xaxis: {
        labels: {
          style: {
            fontSize: "10px",
            lineHeight: "16px",
            fontFamily: "Roboto",
            fontWeight: 300,
            cssClass: "_3months"
          }
        }
      },
      colors: [
        "#2E93fA",
        "#66DA26",
        "#546E7A",
        "#E91E63",
        "#FF9800",
        "#2E294E"
      ],
      dataLabels: { enabled: false }
    };
    new ApexCharts(
      document.querySelector("#<%= key %>-total"),
      options
    ).render();
    document
      .querySelector(".ruby-total-ranges")
      .parentElement.classList.toggle("hidden");
  }
});
