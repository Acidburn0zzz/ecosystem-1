ApexCharts.exec(
  '<%= @total ? "totals-" : "" %>chart-<%= request.path.parameterize %>',
  "updateSeries",
  JSON.parse(`<%= raw(@series.to_json) %>`),
  true
);
ApexCharts.exec(
  '<%= @total ? "totals-" : "" %>chart-<%= request.path.parameterize %>',
  "updateOptions",
  {
    xaxis: {
      labels: {
        style: {
          cssClass: "_<%= @range.key %>"
        }
      }
    }
  }
);
